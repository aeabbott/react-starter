{
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "max-len": ["error", 120],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "globals": {
    "Headers": true,
    "window": true,
    "document": true
  }
}



